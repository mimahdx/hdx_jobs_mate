{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter iOS - Dev",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define=ENV=dev"
      ]
    },
    {
      "name": "Flutter iOS - Staging",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define=ENV=staging"
      ]
    },
    {
      "name": "Flutter iOS - Prod",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--dart-define=ENV=prod"
      ]
    },
    {
      "name": "Flutter iOS - Hotfix",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define=ENV=hotfix"
      ],
      "deviceId": "iOS"
    }
  ]
}
